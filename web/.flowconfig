[ignore]
.*node_modules/redux-token-auth/lib/sagas.js
.*node_modules/react-tap-event-plugin/.*
.*node_modules/react-side-effect/.*
.*node_modules/editions/.*
.*/node_modules/react/.*
.*/node_modules/babel.*
.*/node_modules/fbjs/.*
.*test
.*node_modules/react-grid-layout/build/.*

[include]

[libs]
./types/global.js
./node_modules/redux-token-auth/lib/Types.js

[options]
suppress_comment=\\(.\\|\n\\)*\\s*\\$FlowFixMe.*
suppress_comment=\\(.\\|\n\\)*\\s*\\$FlowBug.*
suppress_comment=\\(.\\|\n\\)*\\s*\\$FlowIgnore.*
suppress_comment=\\(.\\|\n\\)*\\s*\\$FlowNewLine.*
suppress_comment=\\(.\\|\n\\)*\\s*\\$FlowIssue

module.name_mapper='.*\(.css\)' -> 'CSSModule'
module.system=haste
strip_root=true

esproposal.class_instance_fields=ignore
esproposal.class_static_fields=ignore